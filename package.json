{
    "scripts": {
        "frontend:install": "cd udagram/udagram-frontend && npm install --legacy-peer-deps",
        "backend:install": "cd udagram/udagram-api && npm install",
        "frontend:build": "cd udagram/udagram-frontend && NODE_OPTIONS=--openssl-legacy-provider && npm run build",
        "backend:build": "cd udagram/udagram-api && npm run build",
        "frontend:deploy": "cd udagram/udagram-frontend && npm run deploy",
        "backend:deploy": "cd udagram/udagram-api && npm run deploy",
        "backend:aws-eb": "cd udagram/udagram-api && npm run aws-eb",
        "backend:change-main": "cd udagram/udagram-api && npm run change-main",
        "frontend:test": "cd udagram/udagram-frontend && npm run test",
        "frontend:e2e": "cd udagram/udagram-frontend && npm run e2e",
        "frontend:karmaCli": "cd udagram/udagram-frontend && npm run karmaCli"
    }
}
